[package]
build_command = /bin/bash ./build.sh "%(BROKERD_BIND_PORT)" "%(URL)s" "%(PYTHON)s"
script_dir = scripts
user_vars =
   URL [http://localhost:8000] "eZeeKonfigurator URL"
   PYTHON [/usr/bin/env python3] "Path to Python. Note: Python3 is required."
   BROKERD_BIND_PORT [47000] "Port for brokerd to listen on, for manager <-> brokerd communication."
